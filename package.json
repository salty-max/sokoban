{
  "name": "phaser-ts",
  "version": "1.0.0",
  "description": "A boilerplate for Phaser 3 and Typescript",
  "repository": "git+https://github.com/salty-max/phaser-ts.git",
  "author": "Maxime <salty-max> Blanc>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack --mode development && webpack serve --mode development",
    "build": "webpack --mode production",
    "lint": "yarn run lint:eslint && yarn run lint:prettier",
    "lint:eslint": "eslint ./src --ext .js,.ts,.tsx --max-warnings=0",
    "lint:ts": "tsc",
    "lint:prettier": "prettier --debug-check '*/**/*.{js,ts,tsx}'",
    "format": "prettier --write '*/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "css-loader": "^6.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "expose-loader": "^3.1.0",
    "mini-css-extract-plugin": "^2.4.3",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "dependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "phaser": "^3.55.2"
  }
}
