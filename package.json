{
  "name": "phaser-ts",
  "version": "2.0.0",
  "description": "A boilerplate for Phaser 3 and Typescript",
  "author": "Maxime <salty-max> Blanc>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salty-max/phaser-ts.git"
  },
  "homepage": "https://github.com/salty-max/phaser-ts#readme",
  "bugs": {
    "url": "https://github.com/salty-max/phaser-ts/issues"
  },
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "lint": "yarn run lint:eslint && yarn run lint:prettier",
    "lint:eslint": "eslint ./src --ext .js,.ts,.tsx --max-warnings=0",
    "lint:ts": "tsc",
    "lint:prettier": "prettier --debug-check '*/**/*.{js,ts,tsx}'",
    "format": "prettier --write '*/**/*.{js,ts,tsx}'",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "expose-loader": "^3.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^2.4.3",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "dependencies": {
    "phaser": "^3.55.2"
  }
}
